def testLessThan() -> (field):
  1 == lessThan(10, 20)
  0 == lessThan(20, 10)
  return 1

def testECMul() -> (field):
  field[2] G = G()
  field[2] H = H()
  context = context()
  field[2] rG1 = ecMul(1,G)
  field[2] rG2 = ecMul(2,G)
  field[2] vH1 = ecMul(3,H)
  field[2] vH2 = ecMul(4,H)
  1 == onCurve(rG1, context)
  1 == onCurve(rG2, context)
  1 == onCurve(vH1, context)
  1 == onCurve(vH2, context)
  return 1

